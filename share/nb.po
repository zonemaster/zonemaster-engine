#. ZONE:MULTIPLE_SOA
#, perl-brace-format
msgid ""
"Nameserver {ns}/{address} responds with multiple ({count}) SOA records on "
"SOA queries."
msgstr "Navnetjener {ns}/{address} svarte med fleire ({count}) SOA-poster."

#. ZONE:NO_SOA_IN_RESPONSE
#, perl-brace-format
msgid ""
"Response from nameserver {ns}/{address} on SOA queries does not contain SOA "
"record."
msgstr ""
"Svar fra navnetjener {ns}/{address} på spørring om SOA innholdt ingen\n"
"SOA-post."

#. ZONE:MNAME_HAS_NO_ADDRESS
#, perl-brace-format
msgid "No IP address found for SOA 'mname' nameserver ({mname})."
msgstr ""
"Ingen IP-addresse funnet for serveren angitt av SOA/MNAME-post ({mname})."

#. ZONE:ONE_SOA
msgid "A unique SOA record is returned by all nameservers of the zone."
msgstr "En unik SOA-post ble returnert av alle navnetjenere for sonen."

#. ZONE:EXPIRE_MINIMUM_VALUE_OK
#, perl-brace-format
msgid ""
"SOA 'expire' value ({expire}) is higher than the minimum recommended value "
"({required_expire}) and not lower than the 'refresh' value ({refresh})."
msgstr ""
"Verdien på SOA/EXPIRE ({expire}) er høyer enn rekommendert minimum\n"
"({required_expire}) og ikke lavere enn rekommendert \"refresh\" verdi\n"
"({refresh})."

#. ZONE:WRONG_SOA
#, perl-brace-format
msgid ""
"Nameserver {ns}/{address} responds with a wrong owner name ({owner} instead "
"of {name}) on SOA queries."
msgstr ""
"Navnetjener {ns}/{address} svarte med feil \"owner name\" i svaret,\n"
"({owner}) i stedenfor {name} på SOA-spørring."

#. SYSTEM:CANNOT_CONTINUE
#, perl-brace-format
msgid "Not enough data about {zone} was found to be able to run tests."
msgstr ""
"Fant ikke tilstrekkelig med data for sonen {zone} for å kunne kjøre tester."

#. SYSTEM:PROFILE_FILE
#, perl-brace-format
msgid "Profile was read from {name}."
msgstr "Profilen ble lest fra {name}."

#. SYSTEM:DEPENDENCY_VERSION
#, perl-brace-format
msgid "Using prerequisite module {name} version {version}."
msgstr "Bruker modulen {name} med versjon {version} som en forutsetning."

#. SYSTEM:GLOBAL_VERSION
#, perl-brace-format
msgid "Using version {version} of the Zonemaster engine."
msgstr "Bruker versjon {version} av Zonemaster engine."

#. SYSTEM:LOGGER_CALLBACK_ERROR
#, perl-brace-format
msgid "Logger callback died with error: {exception}"
msgstr ""
"Callback-funksjonen for logg-modulen stoppet med feilmelding:  {exception}"

#. SYSTEM:LOOKUP_ERROR
#, perl-brace-format
msgid ""
"DNS query to {ns} for {name}/{type}/{class} failed with error: {message}"
msgstr ""
"DNS-spørring til {ns} for {name}/{type}/{class} misslyktes med\n"
"følgende feil: {message}"

#. SYSTEM:MODULE_ERROR
#, perl-brace-format
msgid "Fatal error in {module}: {msg}"
msgstr "Fatal feil i {module}: {msg}"

#. SYSTEM:MODULE_VERSION
#, perl-brace-format
msgid "Using module {module} version {version}."
msgstr "Bruker modulen {module} med versjon {version}."

#. SYSTEM:MODULE_END
#, perl-brace-format
msgid "Module {module} finished running."
msgstr "Modul {module} avluttet."

#. SYSTEM:NO_NETWORK
msgid "Both IPv4 and IPv6 are disabled."
msgstr "Både IPv4 og IPv6 er deaktivert."

#. SYSTEM:UNKNOWN_METHOD
#, perl-brace-format
msgid "Request to run unknown method {method} in module {module}."
msgstr "Prøvde å kjøre en ukjent testmetode {method} i modulen {module}."

#. SYSTEM:UNKNOWN_MODULE
#, perl-brace-format
msgid ""
"Request to run {method} in unknown module {module}. Known modules: {known}."
msgstr ""
"Prøvde å kjøre en testmetode {method} i en ukjent modul {module}.\n"
"Kjente moduler: {known}."

#. SYSTEM:SKIP_IPV4_DISABLED
#, perl-brace-format
msgid "IPv4 is disabled, not sending query to {ns}."
msgstr "IPv4 er deaktivert, sender ikke spørring til {ns}."

#. SYSTEM:SKIP_IPV6_DISABLED
#, perl-brace-format
msgid "IPv6 is disabled, not sending query to {ns}."
msgstr "IPv6 er deaktivert, sender ikke spørring til {ns}."

#. SYSTEM:FAKE_DELEGATION
msgid "Followed a fake delegation."
msgstr "Fulgte en falsk delegering."

#. SYSTEM:ADDED_FAKE_DELEGATION
#, perl-brace-format
msgid "Added a fake delegation for domain {domain} to name server {ns}."
msgstr "La til en falsk delegering for domenet {domain} til navnetjener {ns}."

#. SYSTEM:FAKE_DELEGATION_TO_SELF
#, perl-brace-format
msgid ""
"Name server {ns} not adding fake delegation for domain {domain} to itself."
msgstr ""
"Navnetjener {ns} la ikke til en skapad delegering for domene {domain} til "
"seg selv."

#. SYSTEM:FAKE_DELEGATION_IN_ZONE_NO_IP
#, perl-brace-format
msgid ""
"The fake delegation of domain {domain} includes an in-zone name server {ns} "
"without mandatory glue (without IP address)."
msgstr ""
"Forsøksdelegeringen av domenet {domain} innholder en navnetjener {ns} som\n"
"trenger limposter."

#. SYSTEM:FAKE_DELEGATION_NO_IP
#, perl-brace-format
msgid ""
"The fake delegation of domain {domain} includes a name server {ns} that "
"cannot be resolved to any IP address."
msgstr ""
"Forsøksdelegeringen av domenet {domain} innholder en navnetjener {ns} som\n"
"ikke slåes opp til noen IP-addresse."

#. SYSTEM:PACKET_BIG
#, perl-brace-format
msgid ""
"Packet size ({size}) exceeds common maximum size of {maxsize} bytes (try "
"with \"{command}\")."
msgstr ""
"Pakkestørrelsen ({size}) byte overskrider maksimumsstørrelse {maxsize} byte\n"
"(prøv med \"{command}\")."
