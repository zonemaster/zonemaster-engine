#: ../lib/Zonemaster/Engine/Test/Syntax.pm:241
#: ../lib/Zonemaster/Engine/Test/Zone.pm:365
#: ../lib/Zonemaster/Engine/Translator.pm:71
#, perl-brace-format
msgid "IPv6 is disabled, not sending \"{rrtype}\" query to {ns}."
msgstr "IPv6 je onemogočen, \"{rrtype}\" ni bila poslana na {ns}."

#. BASIC:IPV6_ENABLED
#: ../lib/Zonemaster/Engine/Test/Basic.pm:314
#, perl-brace-format
msgid "IPv6 is enabled, can send \"{rrtype}\" query to {ns}."
msgstr "IPv6 je omogočen, \"{rrtype}\" poizvedbo lahko pošljemo na {ns}."

#. BASIC:NO_A_RECORDS
#: ../lib/Zonemaster/Engine/Test/Basic.pm:318
#, perl-brace-format
msgid "Nameserver {ns} did not return \"A\" record(s) for {domain}."
msgstr "Strežnik {ns} ni vrnil \"A\" zapisa za '{domain}'."

#. CONNECTIVITY:CONNECTIVITY01
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:159
msgid "UDP connectivity"
msgstr "Povezljivost UDP"

#. CONNECTIVITY:CONNECTIVITY02
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:163
msgid "TCP connectivity"
msgstr "Povezljivost TCP"

#. CONNECTIVITY:CONNECTIVITY03
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:167
msgid "AS Diversity"
msgstr "Raznolikost AS"

#. CONNECTIVITY:CONNECTIVITY04
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:171
msgid "IP Prefix Diversity"
msgstr "Raznolikost IP prefixov"

#. CONNECTIVITY:CN01_IPV4_DISABLED
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:175
#, perl-brace-format
msgid ""
"IPv4 is disabled. No DNS queries are sent to these name servers: "
"\"{ns_list}\"."
msgstr "IPv4 je onemogočen. DNS poizvedbe niso bile poslane na: \"{ns_list}\"."

#. CONNECTIVITY:CN01_IPV6_DISABLED
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:179
#, perl-brace-format
msgid ""
"IPv6 is disabled. No DNS queries are sent to these name servers: "
"\"{ns_list}\"."
msgstr "IPv6 je onemogočen. DNS poizvedbe niso bile poslane na: \"{ns_list}\"."

#. CONNECTIVITY:CN01_MISSING_NS_RECORD_UDP
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:183
#, perl-brace-format
msgid ""
"Nameserver {ns} responds to a NS query with no NS records in the answer "
"section over UDP."
msgstr ""
"Strežnik {ns} je odgovoril na \"NS\" poizvedbo brez NS zapisov v odgovoru "
"prek UDP."

#. CONNECTIVITY:CN01_MISSING_SOA_RECORD_UDP
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:187
#, perl-brace-format
msgid ""
"Nameserver {ns} responds to a SOA query with no SOA records in the answer "
"section over UDP."
msgstr ""
"Strežnik {ns} je odgovoril na \"SOA\" poizvedbo brez SOA zapisov v odgovoru "
"prek UDP."

#. CONNECTIVITY:CN01_NO_RESPONSE_NS_QUERY_UDP
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:191
#, perl-brace-format
msgid "Nameserver {ns} does not respond to NS queries over UDP."
msgstr "Strežnik {ns} ne odgovarja na \"NS\" poizvedbe prek UDP."

#. CONNECTIVITY:CN01_NO_RESPONSE_SOA_QUERY_UDP
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:195
#, perl-brace-format
msgid "Nameserver {ns} does not respond to SOA queries over UDP."
msgstr "Strežnik {ns} ne odgovarja na \"SOA\" poizvedbe prek UDP."

#. CONNECTIVITY:CN01_NO_RESPONSE_UDP
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:199
#, perl-brace-format
msgid "Nameserver {ns} does not respond to any queries over UDP."
msgstr "Strežnik {ns} ne odgovarja na poizvedbe prek UDP."

#. CONNECTIVITY:CN01_NS_RECORD_NOT_AA_UDP
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:203
#, perl-brace-format
msgid ""
"Nameserver {ns} does not give an authoritative response on an NS query over "
"UDP."
msgstr ""
"Strežnik {ns} ne vrača avtoritativnega odgovora na \"NS\" poizvedbe prek UDP."

#. CONNECTIVITY:CN01_SOA_RECORD_NOT_AA_UDP
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:207
#, perl-brace-format
msgid ""
"Nameserver {ns} does not give an authoritative response on an SOA query over "
"UDP."
msgstr ""
"Strežnik {ns} ne vrača avtoritativnega odgovora na \"SOA\" poizvedbe prek "
"UDP."

#. CONNECTIVITY:CN01_UNEXPECTED_RCODE_NS_QUERY_UDP
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:211
#, perl-brace-format
msgid ""
"Nameserver {ns} responds with an unexpected RCODE ({rcode}) on an NS query "
"over UDP."
msgstr ""
"Strežnik {ns} je odgovoril z nepričakovanim RCODE ({rcode}) na \"NS\" "
"poizvedbo prek UDP."

#. CONNECTIVITY:CN01_UNEXPECTED_RCODE_SOA_QUERY_UDP
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:215
#, perl-brace-format
msgid ""
"Nameserver {ns} responds with an unexpected RCODE ({rcode}) on an SOA query "
"over UDP."
msgstr ""
"Strežnik {ns} je odgovoril z nepričakovanim RCODE ({rcode}) na \"SOA\" "
"poizvedbo prek UDP."

#. CONNECTIVITY:CN01_WRONG_NS_RECORD_UDP
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:219
#, perl-brace-format
msgid ""
"Nameserver {ns} responds with a wrong owner name ({domain_found} instead of "
"{domain_expected}) on NS queries over UDP."
msgstr ""
"Strežnik {ns} je odgovoril z nepričakovano domeno ({domain_found}) namesto "
"{domain_expected}) za NS poizvedbe prek UDP."

#. CONNECTIVITY:CN01_WRONG_SOA_RECORD_UDP
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:223
#, perl-brace-format
msgid ""
"Nameserver {ns} responds with a wrong owner name ({domain_found} instead of "
"{domain_expected}) on SOA queries over UDP."
msgstr ""
"Strežnik {ns} je odgovoril z nepričakovano domeno ({domain_found}) namesto "
"{domain_expected}) na SOA poizvedbe prek UDP."

#. CONNECTIVITY:CN02_MISSING_NS_RECORD_TCP
#: ../lib/Zonemaster/Engine/Test/Connectivity.pm:227
#, perl-brace-format
msgid ""
"Nameserver {ns} responds to a NS query with no NS records in the answer "
"section over TCP."
msgstr ""
"Strežnik {ns} je na NS poizvedbo odgovoril brez NS zapisov v odgovoru prek "
