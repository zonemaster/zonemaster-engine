#: ../lib/Zonemaster/Engine/Normalization/Error.pm:29
#, perl-brace-format
msgid ""
"Ambiguous downcasing of character \"{unicode_name}\" in the domain name. Use "
"all lower case instead."
msgstr ""
"Dvoumno zapisovanje znakov \"{unicode_name}\" v imenu domene. Namesto tega "
"uporabite male črke."

#: ../lib/Zonemaster/Engine/Normalization/Error.pm:33
msgid "Domain name is too long (more than 253 characters with no final dot)."
msgstr "Domena je predolga (več kot 253 znakov brez končne pike)."

#: ../lib/Zonemaster/Engine/Normalization/Error.pm:36
msgid "Domain name is empty."
msgstr "Domena je prazna."

#: ../lib/Zonemaster/Engine/Normalization/Error.pm:39
msgid "Domain name starts with dot."
msgstr "Domena se začne s piko."

#: ../lib/Zonemaster/Engine/Normalization/Error.pm:42
#, perl-brace-format
msgid ""
"Domain name has an ASCII label (\"{label}\") with a character not permitted."
msgstr "Domena ima oznako ASCII (\"{label}\") z znakom, ki ni dovoljen."

#: ../lib/Zonemaster/Engine/Normalization/Error.pm:46
#, perl-brace-format
msgid ""
"Domain name has a non-ASCII label (\"{label}\") which is not a valid U-label."
msgstr "Domena ima ne-ASCII oznako (\"{label}\"), ki ni veljaven U-label."

#: ../lib/Zonemaster/Engine/Normalization/Error.pm:50
msgid "Domain name has repeated dots."
msgstr "Domena ima ponavljajujoče se pike."

#: ../lib/Zonemaster/Engine/Normalization/Error.pm:53
#, perl-brace-format
msgid ""
"Domain name has a label that is too long (more than 63 characters), "
"\"{label}\"."
msgstr "Domena ima predolgo oznako (več kot 63 znakov), \"{label}\"."

#. ADDRESS:ADDRESS01
#: ../lib/Zonemaster/Engine/Test/Address.pm:118
msgid "Name server address must be globally routable"
msgstr "Naslov imenskega strežnika mora biti globalno usmerljiv"

#. ADDRESS:ADDRESS02
#: ../lib/Zonemaster/Engine/Test/Address.pm:122
msgid "Reverse DNS entry exists for name server IP address"
msgstr "Za IP-naslov imenskega strežnika obstaja reverse DNS zapis"

#. ADDRESS:ADDRESS03
#: ../lib/Zonemaster/Engine/Test/Address.pm:126
msgid "Reverse DNS entry matches name server name"
msgstr "Reverse DNS se ujema z imenskim strežnikom"

#. ADDRESS:NAMESERVER_IP_WITHOUT_REVERSE
#: ../lib/Zonemaster/Engine/Test/Address.pm:130
#, perl-brace-format
msgid "Nameserver {nsname} has an IP address ({ns_ip}) without PTR configured."
msgstr "Imenski strežnik {nsname} ima IP-naslov ({ns_ip}) brez \"PTR\" zapisa."

#. ADDRESS:NAMESERVER_IP_PTR_MISMATCH
#: ../lib/Zonemaster/Engine/Test/Address.pm:134
#, perl-brace-format
msgid ""
"Nameserver {nsname} has an IP address ({ns_ip}) with mismatched PTR result "
"({names})."
msgstr ""
"Imenski strežnik {nsname} ima IP-naslov ({ns_ip}), ki se ne ujema z \"PTR\" "
"zapisi ({names}) zanj."

#. ADDRESS:NAMESERVER_IP_PRIVATE_NETWORK
#: ../lib/Zonemaster/Engine/Test/Address.pm:138
#, perl-brace-format
msgid ""
"Nameserver {nsname} has an IP address ({ns_ip}) with prefix {prefix} "
"referenced in {reference} as a '{name}'."
msgstr ""
"Imenski strežnik {nsname} z IP-naslovom ({ns_ip}) ima prefix {prefix}, ki je "
"naveden v {reference} kot '{name}'."

#. ADDRESS:NO_IP_PRIVATE_NETWORK
#: ../lib/Zonemaster/Engine/Test/Address.pm:144
msgid "All Nameserver addresses are in the routable public addressing space."
msgstr "Vsi naslovi imenskih strežnikov so v javnem naslovnem prostoru."

#. ADDRESS:NAMESERVERS_IP_WITH_REVERSE
#: ../lib/Zonemaster/Engine/Test/Address.pm:148
msgid "Reverse DNS entry exists for each Nameserver IP address."
msgstr "Vsi IP-naslovi imenskega strežnika imajo reverse DNS zapis."

#. ADDRESS:NAMESERVER_IP_PTR_MATCH
#: ../lib/Zonemaster/Engine/Test/Address.pm:152
msgid "Every reverse DNS entry matches name server name."
msgstr "Vsak reverse DNS se ujema z imenom imenskega strežnika."

#. ADDRESS:NO_RESPONSE_PTR_QUERY
#: ../lib/Zonemaster/Engine/Test/Address.pm:156
#, perl-brace-format
msgid "No response from nameserver(s) on PTR query ({domain})."
msgstr "Ni odgovora od imenskega strežnika za \"PTR\" ({domain})."

